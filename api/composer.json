{
    "name": "e7d/neap",
    "description": "A ready to go streaming platform.",
    "type": "software",
    "keywords": ["streaming", "rtmp"],
    "homepage": "https://github.com/e7d/neap",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Ferrand",
            "homepage": "http://e7d.io"
        }
    ],
    "support": {
        "issues": "https://github.com/e7d/neap/issues",
        "source": "https://github.com/e7d/neap"
    },
    "config": {
        "process-timeout": 5000
    },
    "require": {
        "php": ">=5.6",
        "zendframework/zendframework": "~2.4",
        "zfcampus/zf-apigility": "~1.1",
        "zfcampus/zf-apigility-documentation": "^1.0.5",
        "zfcampus/zf-apigility-documentation-swagger": "^1.0.4",
        "zfcampus/zf-development-mode": "~2.0",
        "react/react": "^0.4.2"
    },
    "require-dev": {
        "phpunit/phpunit": "4.0.7",
        "squizlabs/php_codesniffer": "2.2.*",
        "jakub-onderka/php-parallel-lint": "0.8.*",
        "satooshi/php-coveralls": "0.6.*",
        "zendframework/zftool": "dev-master",
        "zendframework/zend-developer-tools": "dev-master",
        "zendframework/zend-test": "2.5.1",
        "zfcampus/zf-apigility-admin": "^1.3.1",
        "zfcampus/zf-deploy": "~1.0"
    },
    "suggest": {
        "zfcampus/zf-apigility-doctrine": "zfcampus/zf-apigility-doctrine ~1.0 to create Doctrine-Connected REST services",
        "zfcampus/zf-http-cache": "zfcampus/zf-http-cache ~1.0 to add HTTP caching to your API",
        "zfr/zfr-cors": "zfr/zfr-cors ~1.2 to add CORS support to your API"
    }
}
